<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.NoActivityStarter" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primaryColor</item>
        <item name="colorPrimaryVariant">@color/primaryLightColor</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/secondaryColor</item>
        <item name="colorSecondaryVariant">@color/secondaryDarkColor</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimary</item>
        <!-- Customize your theme here. -->
        <item name="textInputStyle">@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense</item>
        <item name="textInputFilledDenseStyle">@style/Widget.Material3.TextInputEditText.FilledBox.Dense</item>
        <item name="colorButtonNormal">@color/lightGreen1</item>

    </style>
    <style name="ShapeAppearanceOverlay.App.CornerSize50Percent" parent="">
        <item name="cornerSize">50%</item>
    </style>
    <style name="ShapeAppearanceOverlay.App.CornerSize25Percent" parent="">
        <item name="cornerSize">25%</item>
    </style>
    <style name="ShapeAppearanceOverlay.App.CornerSize15Percent" parent="">
        <item name="cornerSize">15%</item>
    </style>
    <style name="DialogRoundedCorner" parent="@style/Theme.MaterialComponents.Dialog">
        <item name="dialogCornerRadius">12dp</item>
    </style>


</resources>